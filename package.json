{
  "name": "cloudfuse-platform",
  "version": "1.0.0",
  "description": "CloudFuse Platform - A PaaS solution for deploying applications",
  "main": "dist/main.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "swagger": "nest start --watch & npm run swagger:generate",
    "swagger:generate": "ts-node -r tsconfig-paths/register src/generate-swagger.ts",
    "migrate": "ts-node -r tsconfig-paths/register src/migrate.ts",
    "migrate:generate": "typeorm-ts-node-commonjs migration:generate -d src/config/database.config.ts",
    "migrate:create": "typeorm-ts-node-commonjs migration:create",
    "migrate:run": "typeorm-ts-node-commonjs migration:run -d src/config/database.config.ts",
    "migrate:revert": "typeorm-ts-node-commonjs migration:revert -d src/config/database.config.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manziosee/cloudfuse-platform.git"
  },
  "keywords": [
    "paas",
    "cloud",
    "deployment",
    "docker",
    "kubernetes"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/manziosee/cloudfuse-platform/issues"
  },
  "homepage": "https://github.com/manziosee/cloudfuse-platform#readme",
  "dependencies": {
    "@kubernetes/client-node": "^1.3.0",
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.1.5",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@supabase/supabase-js": "^2.53.0",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dockerode": "^4.0.7",
    "dotenv": "^16.3.1",
    "helmet": "^8.0.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "prom-client": "^15.1.3",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "tar-fs": "^3.0.4",
    "tmp": "^0.2.1",
    "typeorm": "^0.3.17",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.8",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^11.1.5",
    "@types/bcrypt": "^5.0.1",
    "@types/dockerode": "^3.3.17",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.6",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^20.8.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.12",
    "@types/tar-fs": "^2.0.4",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  }
}
